{
  "name": "vite-ts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "preview:dev": "vite --mode development",
    "preview:prd": "vite --mode production",
    "build:ts:dev": "vue-tsc --noEmit && vite build --mode development",
    "build:ts:prd": "vue-tsc --noEmit && vite build --mode production",
    "build:dev": "vite build --mode development",
    "build:prd": "vite build --mode production",
    "lint:ext": "eslint --ext .js,.jsx,.ts,.tsx,.vue src/",
    "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx,.vue src/",
    "lint:lint-staged": "lint-staged",
    "fix:prettier": "prettier --write  \"src/**/*.{js,jsx,ts,tsx,vue,css,scss,html,json}\"",
    "prepare": "husky install",
    "commit": "cz"
  },
  "dependencies": {
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@commitlint/cz-commitlint": "^17.2.0",
    "axios": "^0.27.2",
    "commitizen": "^4.2.5",
    "js-md5": "^0.7.3",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.22",
    "store": "^2.0.12",
    "vue": "^3.2.37",
    "vue-router": "4"
  },
  "devDependencies": {
    "@arco-design/web-vue": "^2.36.1",
    "@types/js-md5": "^0.4.3",
    "@types/node": "^18.7.16",
    "@types/nprogress": "^0.2.0",
    "@types/sortablejs": "^1.15.1",
    "@types/store": "^2.0.2",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "@vitejs/plugin-vue": "^3.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.9.0",
    "husky": "^8.0.3",
    "less": "^4.1.3",
    "lint-staged": "^13.2.0",
    "prettier": "^2.8.4",
    "sortablejs": "^1.15.0",
    "typescript": "^4.9.5",
    "unplugin-auto-import": "^0.11.2",
    "unplugin-vue-components": "^0.22.4",
    "vite": "^3.1.0",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-mock": "^2.9.6",
    "vue-tsc": "^0.40.4"
  }
